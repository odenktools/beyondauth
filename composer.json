{
  "name": "pribumi/beyondauth",
  "description": "Complete User Auth For Laravel 5.2.x",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "auth",
    "authentication",
    "beyondauth",
    "laravel"
  ],
  "homepage": "https://github.com/odenktools/beyondauth",
  "authors": [{
    "name": "pribumi",
    "email": "support@pribumitech.com",
    "homepage": "http://docs.pribumitech.com/beyondauth",
    "role": "Developer"
  }],
  "require": {
    "php": "^5.5.9 || ^7.0",
    "illuminate/contracts": "5.2.* || 5.3.* || 5.4.* || 5.5.*",
    "illuminate/support": "5.2.* || 5.3.* || 5.4.* || 5.5.*",
    "illuminate/validation": "5.2.* || 5.3.* || 5.4.* || 5.5.*",
    "nesbot/carbon": "~1.18"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.1",
    "mockery/mockery": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Pribumi\\BeyondAuth\\": "src/BeyondAuth/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
